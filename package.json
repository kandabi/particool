{
  "name": "particool",
  "private": true,
  "version": "0.0.2",
  "main": "./dist/particool.cjs",
  "module": "./dist/particool.mjs",
  "exports": {
    ".": {
      "require": "./dist/particool.cjs",
      "import": "./dist/particool.mjs"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "rm -rf dist && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "format:scripts": "prettier . --write",
    "format": "pnpm format:scripts ",
    "prepare": "husky install && husky set .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "uninstall-husky": "pnpm uninstall husky --no-save && git config --unset core.hooksPath && npx rm -rf .husky"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitest/coverage-v8": "^0.34.1",
    "dts-bundle-generator": "^9.2.4",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  }
}
